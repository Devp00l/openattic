    "eslint-plugin-angular": "3.1.0",
    "grunt": "0.4.5",
    "grunt-babel": "6.0.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-concat": "1.0.0",
    "grunt-contrib-copy": "0.8.2",
    "grunt-contrib-cssmin": "0.14.0",
    "grunt-contrib-uglify": "0.11.1",
    "grunt-contrib-watch": "0.6.1",
    "grunt-eslint": "^20.0.0",
    "grunt-html-build": "0.5.3",
    "grunt-ng-annotate": "1.0.1",
    "grunt-rev": "0.1.0",
    "grunt-usemin": "3.1.1",
    "load-grunt-tasks": "3.4.1",
    "matchdep": "1.0.1",
    "protractor": "5.1.2",
    "protractor-jasmine2-screenshot-reporter": "0.4.0"
  },
  "engines": {
    "node": ">=4.3.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules/ dist/bower_components/ app/bower_components/",
    "build": "npm run build:npm && npm run build:bower && npm run build:grunt",
    "build:npm": "npm install -g bower grunt-cli && npm install",
    "build:bower": "bower install --allow-root",
    "build:grunt": "grunt build",
    "rebuild": "npm run clean && npm run build",
    "webdriver": "webdriver-manager update && webdriver-manager start",
    "protractor": "protractor protractor.conf.js"
  }
